{
  "name": "@tduniec/backstage-timesaver-plugin",
  "version": "1.0.0",
  "engines": {
    "node": "18 || 20"
  },
  "workspaces": {
    "packages": [
      "plugins/*"
    ]
  },
  "repository": "https://github.com/tduniec/backstage-timesaver-plugin.git",
  "author": "tduniec <tomekduniec@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "build": "yarn workspaces run build",
    "clean": "yarn workspaces run clean",
    "tsc": "tsc",
    "test": "backstage-cli repo test",
    "test:all": "backstage-cli repo test --coverage",
    "fix": "backstage-cli repo fix",
    "lint": "backstage-cli repo lint --since origin/main",
    "lint:all": "backstage-cli repo lint",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier . --write"
  },
  "devDependencies": {
    "@backstage/cli": "^0.25.1",
    "@backstage/codemods": "workspace:*",
    "@backstage/create-app": "workspace:*",
    "@backstage/repo-tools": "workspace:*",
    "@changesets/cli": "^2.14.0",
    "@octokit/rest": "^19.0.3",
    "@playwright/test": "^1.32.3",
    "@spotify/eslint-plugin": "^14.1.3",
    "@spotify/prettier-config": "^14.0.0",
    "@techdocs/cli": "workspace:*",
    "@types/node": "^18.17.8",
    "@types/webpack": "^5.28.0",
    "command-exists": "^1.2.9",
    "concurrently": "^8.0.0",
    "cross-env": "^7.0.0",
    "eslint": "^8.6.0",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-testing-library": "^6.0.0",
    "fs-extra": "10.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.0",
    "minimist": "^1.2.5",
    "node-gyp": "^10.0.0",
    "prettier": "^2.2.1",
    "semver": "^7.5.3",
    "shx": "^0.3.2",
    "ts-node": "^10.4.0",
    "typescript": "~5.2.0"
  },
  "description": "This plugin provides an implementation of charts and statistics related to your time savings that are coming from usage of your templates.",
  "private": true
}
